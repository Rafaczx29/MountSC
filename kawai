-- Toggle ikon
local toggleBtn = Instance.new("ImageButton")
toggleBtn.Size = UDim2.new(0, 40, 0, 40)
toggleBtn.Position = UDim2.new(0, 10, 0.05, 0)
toggleBtn.Image = "rbxassetid://3926305904"
toggleBtn.ImageRectOffset = Vector2.new(764, 324)
toggleBtn.ImageRectSize = Vector2.new(36, 36)
toggleBtn.Parent = ScreenGui

MainFrame.Visible = false
toggleBtn.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

-- Logic summit looping
local running = false
local function doSummit(loopCount)
    running = true
    for l = 1, loopCount do
        if not running then break end
        for i, cf in ipairs(checkpoints) do
            if not running then break end
            humanoidRootPart = player.Character and player.Character:WaitForChild("HumanoidRootPart")
            if humanoidRootPart then
                humanoidRootPart.CFrame = cf
            end
            task.wait(2)
            if i == #checkpoints then
                player.Character:BreakJoints() -- reset karakter di akhir summit
                task.wait(5) -- tunggu respawn biar ga langsung lompat CP3
            end
        end
    end
    running = false
end

startBtn.MouseButton1Click:Connect(function()
    if not running then
        local count = tonumber(loopBox.Text) or 1
        doSummit(count)
    end
end)

stopBtn.MouseButton1Click:Connect(function()
    running = false
end)
