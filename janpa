-- // Core Functions
local running = false

local function getHRP()
	local char = player.Character or player.CharacterAdded:Wait()
	return char:WaitForChild("HumanoidRootPart")
end

local function resetCharacter()
	player.Character:BreakJoints()
end

local function startSummit()
	if running then return end
	running = true
	StartBtn.Text = "‚è∏Ô∏è RUNNING..."
	repeat
		for i, cf in ipairs(checkpoints) do
			if not running then break end
			local hrp = getHRP()
			hrp.CFrame = cf
			task.wait(1)
		end
		if running then
			resetCharacter()
			task.wait(6)
		end
	until not running
	StartBtn.Text = "‚ñ∂Ô∏è START"
end

local function stopSummit()
	running = false
	StartBtn.Text = "‚ñ∂Ô∏è START"
end

StartBtn.MouseButton1Click:Connect(startSummit)
StopBtn.MouseButton1Click:Connect(stopSummit)

local visible = true
ToggleBtn.MouseButton1Click:Connect(function()
	visible = not visible
	Frame.Visible = visible
end)
