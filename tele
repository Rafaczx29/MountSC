--// Teleport Custom CFrame UI (Fixed for Delta Executor)
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Pastikan karakter siap
if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
	player.CharacterAdded:Wait()
end

local hrp = player.Character:WaitForChild("HumanoidRootPart")

-- Gunakan gethui() agar UI aktif di executor seperti Delta
local guiParent = gethui and gethui() or game.CoreGui

-- Hapus UI lama jika ada
if guiParent:FindFirstChild("TeleportUI") then
	guiParent.TeleportUI:Destroy()
end

--// Buat UI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "TeleportUI"
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = guiParent

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 280, 0, 150)
Frame.Position = UDim2.new(0.5, -140, 0.5, -75)
Frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
Frame.BorderSizePixel = 0
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 35)
Title.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Title.Text = "üõ∞Ô∏è Teleport ke CFrame"
Title.Font = Enum.Font.GothamBold
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextSize = 16
Title.Parent = Frame

local CloseBtn = Instance.new("TextButton")
CloseBtn.Size = UDim2.new(0, 35, 0, 35)
CloseBtn.Position = UDim2.new(1, -35, 0, 0)
CloseBtn.Text = "‚úï"
CloseBtn.Font = Enum.Font.GothamBold
CloseBtn.TextSize = 16
CloseBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseBtn.BackgroundColor3 = Color
